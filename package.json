{
  "name": "natours",
  "version": "1.0.0",
  "description": "node, express and mongoDB",
  "main": "app.js",
  "targets": {
    "frontend": {
      "source": "public/js/index.js",
      "distDir": "public/js/bundled"
    }
  },
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "start:prod": "cross-env NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch --target frontend",
    "build:js": "parcel build --target frontend",
    "dev": "concurrently \"npm run watch:js\" \"npm run start\"",
    "prod": "concurrently \"npm run watch:js\" \"npm run start:prod\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "mongodb": "^6.11.0",
    "mongoose": "^8.10.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.10.0",
    "pug": "^3.0.3",
    "sharp": "^0.34.1",
    "slugify": "^1.6.6",
    "stripe": "^18.1.1",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@parcel/optimizer-terser": "^2.15.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "nodemon": "^3.0.3",
    "parcel": "^2.15.1",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
